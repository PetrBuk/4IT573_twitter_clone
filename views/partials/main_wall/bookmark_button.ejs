<button type="button" hx-post="/api/tweet/bookmark/<%= tweet.id %>" hx-swap="outerHTML"
	class="w-auto mt-1 group flex gap-1 items-center text-gray-500 px-3 py-2 text-base leading-6 font-medium rounded-full hover:bg-gray-600 hover:text-blue-300">

	<svg class="h-7 w-6" fill="<%= tweet.isBookmarked ? '#93c5fd' : 'none' %>" stroke-linecap="round"
		stroke-linejoin="round" stroke-width="2" stroke="<%= tweet.isBookmarked ? '#93c5fd' : 'currentColor' %>"
		viewBox="0 0 24 24">
		<path d="M5 5a2 2 0 012-2h10a2 2 0 012 2v16l-7-3.5L5 21V5z"></path>
	</svg>

</button>